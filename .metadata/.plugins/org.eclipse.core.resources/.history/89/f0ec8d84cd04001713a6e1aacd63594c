package tp1.ex3.decrypte.impl;

import tp1.ex3.decrypte.DecryptComponent;
import tp1.ex3.decrypte.interfaces.Decrypt;

public class DecryptImpl extends DecryptComponent {

	@Override
	protected Decrypt make_service() {
		return new Decrypt() {
			
			@Override
			public void decrypt(String source) {
				String message = new StringBuilder(source).reverse().toString();
				requires().helper().encapsule(message);
			}
		};
	}

}
