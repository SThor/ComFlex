package tp1.ex3.composite.impl;

import tp1.ex3.client.ClientComponent;
import tp1.ex3.client.impl.ClientImpl;
import tp1.ex3.composite.CompositeComponent;
import tp1.ex3.crypte.CrypteComponent;
import tp1.ex3.crypte.impl.CryptImpl;
import tp1.ex3.decrypte.DecryptComponent;
import tp1.ex3.decrypte.impl.DecryptImpl;
import tp1.ex3.server.ServerComponent;
import tp1.ex3.server.impl.ServerImpl;
import tp1.ex3.spreader.SpreaderComponent;
import tp1.ex3.spreader.impl.SpreaderImpl;

public class CompositeImpl extends CompositeComponent{


	@Override
	protected ClientComponent make_client() {
		return new ClientImpl();
	}


	@Override
	protected ServerComponent make_server1() {
		return new ServerImpl(1);
	}


	@Override
	protected ServerComponent make_server2() {
		return new ServerImpl(2);
	}

	@Override
	protected ServerComponent make_server3() {
		return new ServerImpl(3);
	}

	@Override
	protected SpreaderComponent make_spreader() {
		return new SpreaderImpl();
	}


	@Override
	protected CrypteComponent make_crypt() {
		return new CryptImpl();
	}


	@Override
	protected DecryptComponent make_decrypt1() {
		return new DecryptImpl();
	}


	@Override
	protected DecryptComponent make_decrypt2() {
		return new DecryptImpl();
	}


	@Override
	protected DecryptComponent make_decrypt3() {
		return new DecryptImpl();
	}

}
